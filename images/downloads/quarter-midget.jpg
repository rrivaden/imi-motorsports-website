    padding-top: 0px !important;
}

/* To make event calendar height 215px */
.tribe-common--breakpoint-medium.tribe-events .tribe-events-l-container {
    min-height: 215px !important;
}

/* To make event calendar font size 18px*/
.tribe-common--breakpoint-medium.tribe-common .tribe-common-form-control-text__input {
	font-size: 18px;
}

/* To avoid cropping of h1 */
h1{
	padding-left: 5px;
}

/* To make event calendar font size 18px*/
.tribe-common--breakpoint-medium.tribe-common .tribe-common-h6--min-medium {
	font-size: 18px;
}

/* To make event calendar font size 18px*/
.tribe-common span{
	    font-size: 18px !important;
}

/* To make event calendar font size 18px*/
.tribe-common p {
    font-size: 18px;
}

/* To make border radius 30px as per figma of event calendar "Find Event button"  */
button.tribe-common-c-btn.tribe-events-c-search__button{
	font-weight: 400 !important;
	border-radius: 0px !important;
	border: none !important;
}

/* To change border color of "Find Events" */
button.tribe-common-c-btn.tribe-events-c-search__button {
	border: none !important;
}

/* To change event calendar button color  */
.tribe-events .tribe-events-c-search__button, .tribe-events button.tribe-events-c-search__button, .tribe-common--breakpoint-medium.tribe-common .tribe-common-c-btn-border, .single-tribe_events .tribe-events-c-subscribe-dropdown__button{
	background-color: #ff5722 !important;
	color:#ffffff !important;
	font-size:18px;
	font-family: lato; 
